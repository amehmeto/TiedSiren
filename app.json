{
  "expo": {
    "name": "Tied Siren",
    "slug": "tiedsiren",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tiedsiren.tiedsiren",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "processing"
        ],
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.transistorsoft.fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tiedsiren.tiedsiren",
      "permissions": ["android.permission.QUERY_ALL_PACKAGES"]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "config": {
        "fontFamily": "Roboto"
      }
    },
    "extra": {
      "eas": {
        "projectId": "3886f8c6-d468-4596-b9bb-5ca38b0cd5e2"
      }
    }
  }
}
